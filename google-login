<script type="text/javascript">
  var gaia_attachEvent = function(element, event, callback) {
  if (element.addEventListener) {
  element.addEventListener(event, callback, false);
  } else if (element.attachEvent) {
  element.attachEvent('on' + event, callback);
  }
  };
</script>
  <script>var f=this,g=function(e,d){var a=e.split("."),b=f;a[0]in b||!b.execScript||b.execScript("var "+a[0]);for(var c;a.length&&(c=a.shift());)a.length||void 0===d?b[c]?b=b[c]:b=b[c]={}:b[c]=d};var h=function(){try{return new XMLHttpRequest}catch(e){for(var d=["MSXML2.XMLHTTP.6.0","MSXML2.XMLHTTP.3.0","MSXML2.XMLHTTP","Microsoft.XMLHTTP"],a=0;a<d.length;a++)try{return new ActiveXObject(d[a])}catch(b){}}return null};g("gaia.ajax.newXmlHttpRequest",h);var k=function(){this.a=h();this.parameters={}};g("gaia.ajax.XmlHttpFormRequest",k);
k.prototype.send=function(e,d){var a=[],b;for(b in this.parameters)a.push(b+"="+encodeURIComponent(this.parameters[b]));var a=a.join("&"),c=this.a;c.open("POST",e,!0);c.setRequestHeader("Content-type","application/x-www-form-urlencoded");c.onreadystatechange=function(){4==c.readyState&&d({status:c.status,text:c.responseText})};c.send(a)};k.prototype.send=k.prototype.send;
